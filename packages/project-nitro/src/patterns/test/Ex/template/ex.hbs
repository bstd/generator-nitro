<div class="t-ex{{#if modifier}} t-ex--{{modifier}}{{/if}}" data-t-name="Ex"{{#if decorator}} data-t-decorator="{{decorator}}"{{/if}}>
	<h2 class="t-ex__headline">{{title}}</h2>
	<ul class="js-t-ex__list">
		{{#each links}}
			{{> partial/ex.link}}
		{{/each}}
	</ul>
	<button class="t-ex__button js-t-ex__more">more links</button>
</div>
